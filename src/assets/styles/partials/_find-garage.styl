.find-garage
  color $blue
  .container
    padding 0
    background $white
    width 100%!important
    margin-top 0
  h1
    text-align right
    font-size $font-size-x-large
    font-weight $font-light
    .customer
      font-weight $font-bold
// Garage Controls Footer
.garage-controls
  text-align center
  box-shadow $box-shadow-dark-top
  background $white
  height 300px
  width 100%
  position absolute
  bottom 0
  z-index 1000
  .selectors
    color $blue
    span
      margin-top 10px
      font-size $font-size-large
      display block
